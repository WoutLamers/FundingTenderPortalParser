{"TopicDetails": {"type": 0, "ccm2Id": 42262474, "cftId": 0, "identifier": "COS-2019-3-01", "title": "Boosting sustainable tourism development and capacity of tourism SMEs through transnational cooperation", "publicationDateLong": 1564963200000, "callIdentifier": "COS-TOURCOOP-2019-3-01", "callTitle": "Boosting sustainable tourism development and capacity of tourism SMEs through transnational cooperation and knowledge transfer", "callccm2Id": 42262492, "allowPartnerSearch": true, "workProgrammepart": {"id": 274550, "ccm_id": 31116409, "wp_part": "COSME-2019", "wp_year": "COSME-2019", "wp_title": "COSME 2019", "wp_website": "https://ec.europa.eu/growth/smes/cosme_en", "wp_document": "http://ec.europa.eu/research/participants/data/ref/other_eu_prog/cosme/wp-call/cosme-wp-2019_en.pdf"}, "frameworkProgramme": {"id": 31059643, "abbreviation": "COSME", "description": "Programme for the Competitiveness of Enterprises and small and medium-sized enterprises"}, "programmeDivision": [{"id": 31059644, "abbreviation": "COSME.1", "description": "COSME"}, {"id": 31061068, "abbreviation": "COSME.1.3", "description": "More favourable conditions for business creation and growth"}], "topicMGAs": [], "tags": ["TRANSNATIONAL COOPERATION", "TOURISM", "KNOWLEDGE TRANSFER", "SUSTAINABILITY", "COSME", "SME SUPPORT", "CAPACITY BUILDING"], "sme": false, "actions": [{"status": {"id": 31094503, "abbreviation": "Closed", "description": "Closed"}, "types": [{"typeOfAction": "COSME-GA Grant agreement", "typeOfMGA": []}], "plannedOpeningDate": "07 August 2019", "submissionProcedure": {"id": 31094504, "abbreviation": "single-stage", "description": "single-stage"}, "deadlineDates": ["24 October 2019"]}], "latestInfos": [], "budgetOverviewJSONItem": {"budgetTopicActionMap": {"3274981": [{"action": "COS-2019-3-01 - COSME-GA Grant agreement", "plannedOpeningDate": "07 August 2019", "deadlineModel": "single-stage", "deadlineDates": ["24 October 2019"], "budgetYearMap": {"2019": 5000000}, "budgetTopicActi