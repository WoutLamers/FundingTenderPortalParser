{"TopicDetails": {"type": 0, "ccm2Id": 31101900, "cftId": 0, "identifier": "ISFP-2018-AG-SCHEN", "title": "Schengen", "publicationDateLong": 1540425600000, "callIdentifier": "ISFP-2018-AG-SCHEN", "callTitle": "Call restricted to Member States for projects aimed at furthering the Commission Recommendation on proportionate police checks and police cooperation in the Schengen area ", "callccm2Id": 31101901, "allowPartnerSearch": true, "workProgrammepart": {"id": 270551, "ccm_id": 31103647, "wp_part": "ISFP-2018", "wp_year": "ISFP-2018", "wp_title": "Internal Security Fund Police", "wp_website": "http://ec.europa.eu/dgs/home-affairs/financing/fundings/security-and-safeguarding-liberties/internal-security-fund-police/index_en.htm", "wp_document": "https://ec.europa.eu/research/participants/data/ref/other_eu_prog/home/wp/isfp-awp-2018_en.pdf "}, "frameworkProgramme": {"id": 31077817, "abbreviation": "ISFP", "description": "Internal Security Fund Police"}, "programmeDivision": [{"id": 31077808, "abbreviation": "ISFP-1-1", "description": "Law enforcement"}, {"id": 31077807, "abbreviation": "ISFP-1", "description": "Police cooperation"}, {"id": 31077809, "abbreviation": "ISFP-1-2", "description": "Police cooperation"}], "topicMGAs": [], "tags": ["borders", "Schengen"], "sme": false, "actions": [{"status": {"id": 31094503, "abbreviation": "Closed", "description": "Closed"}, "types": [{"typeOfAction": "ISFP-AG ISF-Police Action Grant", "typeOfMGA": []}], "plannedOpeningDate": "25 October 2018", "submissionProcedure": {"id": 31094504, "abbreviation": "single-stage", "description": "single-stage"}, "deadlineDates": ["02 May 2019"]}], "latestInfos": [], "budgetOverviewJSONItem": {"budgetTopicActionMap": {"3245705": [{"action": "ISFP-2018-AG-SCHEN - ISFP-AG ISF-Police Action Grant", "plannedOpeningDate": "25 October 2018", "deadlineModel": "single-stage", "deadlineDates": ["02 May 2019"], "budgetYearMap": {"2018": 1800000}, "budgetTopicActionMap": {}}]}, "budgetYearsColumns": ["20